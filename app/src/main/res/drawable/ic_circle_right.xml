<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:start="-17dp">
        <shape
            android:shape="oval">

            <solid
                android:color="#F0F0F0"/>

            <size
                android:width="35dp"
                android:height="35dp"/>
        </shape>
    </item>

    <item android:bottom="23dp" android:top="11.5dp" android:end="1.6dp">
        <rotate
            android:fromDegrees="45">
            <shape android:shape="rectangle">
                <solid android:color="@android:color/black"/>
                <corners
                    android:radius="1dp"
                    android:bottomRightRadius="0dp"
                    android:topRightRadius="0dp"/>
            </shape>
        </rotate>
    </item>

    <item android:top="23dp" android:bottom="11.5dp" android:end="1.6dp">
        <rotate
            android:fromDegrees="-45">
            <shape android:shape="rectangle">
                <solid android:color="@android:color/black"/>
                <corners
                    android:radius="1dp"
                    android:bottomRightRadius="0dp"
                    android:topRightRadius="0dp"/>
            </shape>
        </rotate>
    </item>

</layer-list>